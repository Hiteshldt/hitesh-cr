{
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/server.js"
      }
    ],
    "env": {
      "MongoDb_Link": "@mongo_db_link",
      "jwt_key": "@jwt_key"
    }
  }
  